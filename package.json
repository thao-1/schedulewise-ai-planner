{
  "name": "schedulewise-ai-planner",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "engines": {
    "node": ">=16.0.0"
  },
  "scripts": {
    "dev": "concurrently \"npm:dev:frontend\" \"npm:dev:server\"",
    "dev:frontend": "vite",
    "dev:server": "NODE_OPTIONS='--loader ts-node/esm' nodemon --watch 'src/server' -e ts,tsx --exec 'node' src/server/index.ts",
    "build": "vite build",
    "build:server": "tsc -p tsconfig.server.json",
    "build:all": "npm run build && npm run build:server",
    "start": "node -r dotenv/config dist/server/index.js",
    "start:prod": "NODE_ENV=production node -r dotenv/config dist/server/index.js",
    "lint": "eslint . --ext .ts,.tsx,.js,.jsx",
    "lint:fix": "eslint . --ext .ts,.tsx,.js,.jsx --fix",
    "preview": "vite preview",
    "type-check": "tsc --noEmit",
    "type-check:watch": "npm run type-check -- --watch"
  },
  "dependencies": {
